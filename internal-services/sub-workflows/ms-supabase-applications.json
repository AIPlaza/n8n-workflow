    {
      "parameters": {
        "jsCode": "// Analytics computation based on type\nconst analyticsType = $('Parse Input').item.json.analytics_type;\nconst applications = $input.all();\n\nlet result = {};\n\nswitch(analyticsType) {\n  case 'conversion':\n    const total = applications.length;\n    const hired = applications.filter(a => a.json.status === 'hired').length;\n    result = {\n      total_applications: total,\n      hired: hired,\n      conversion_rate: total > 0 ? (hired / total * 100).toFixed(2) + '%' : '0%'\n    };\n    break;\n    \n  case 'top_jobs':\n    const jobCounts = {};\n    applications.forEach(a => {\n      const jobId = a.json.job_id;\n      jobCounts[jobId] = (jobCounts[jobId] || 0) + 1;\n    });\n    result = {\n      top_jobs: Object.entries(jobCounts)\n        .sort((a, b) => b[1] - a[1])\n        .slice(0, 10)\n        .map(([job_id, count]) => ({ job_id, applications: count }))\n    };\n    break;\n    \n  case 'response_rate':\n    const withResponse = applications.filter(a => \n      a.json.status !== 'submitted' && a.json.status !== 'pending'\n    ).length;\n    result = {\n      total: applications.length,\n      with_response: withResponse,\n      response_rate: applications.length > 0 ? \n        (withResponse / applications.length * 100).toFixed(2) + '%' : '0%'\n    };\n    break;\n    \n  case 'trend':\n    // Group by month\n    const months = {};\n    applications.forEach(a => {\n      const month = new Date(a.json.created_at).toISOString().slice(0, 7);\n      months[month] = (months[month] || 0) + 1;\n    });\n    result = {\n      monthly_trend: Object.entries(months)\n        .sort((a, b) => a[0].localeCompare(b[0]))\n        .map(([month, count]) => ({ month, applications: count }))\n    };\n    break;\n}\n\nreturn [{ json: result }];"
      },
