  "description": "TOOL NAME: ms_supabase_profile\n\nDESCRIPTION:\nProvides CRUD operations for candidate profiles. Supports retrieval by ID or\nemail, listing all profiles, and creating or updating profile data.\n\nINPUT SCHEMA:\n{\n  \"type\": \"object\",\n  \"properties\": {\n    \"action\": {\n      \"type\": \"string\",\n      \"enum\": [\"get_by_id\", \"get_by_email\", \"get_all\", \"create\", \"update\", \"delete\"],\n      \"description\": \"Profile operation to perform\"\n    },\n    \"profile_id\": {\n      \"type\": \"string\",\n      \"format\": \"uuid\",\n      \"description\": \"Profile UUID\"\n    },\n    \"email\": {\n      \"type\": \"string\",\n      \"format\": \"email\",\n      \"description\": \"Profile email address\"\n    },\n    \"data\": {\n      \"type\": \"object\",\n      \"description\": \"JSON object with profile fields to create/update\"\n    },\n    \"limit\": {\n      \"type\": \"number\",\n      \"default\": 50,\n      \"description\": \"Number of profiles to return for get_all\"\n    }\n  },\n  \"required\": [\"action\"]\n}"
